{
  "name": "node-config-schema",
  "version": "1.0.3",
  "description": "Type checking for config files with type definition injection for typescript",
  "private": false,
  "main": "dist/lib/index.js",
  "author": "Alexandru Cancescu",
  "scripts": {
    "inject": "node dist/inject.js src/Config.ts"
  },
  "dependencies": {
    "config": "^3.3.3",
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^11.11.0"
  },
  "bin": {
    "tconfig-inject": "./dist/inject.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandrucancescu/node-config-schema"
  },
  "files": [
