  "types": "dist/lib/index.d.ts",
  "typings": "dist/lib/index.d.ts",
  "license": "MIT",
  "keywords": [
    "node",
    "config",
    "schema",
    "typescript",
    "node-config",
    "ts-config",
    "config-files"
